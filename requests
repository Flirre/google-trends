:server = http://localhost:3001
# POST team 1 medium score
POST :server/term?team=team1&searchTerm=test

#POST team 2 high score
POST :server/term?team=team2&searchTerm=google

#GET trend-data
GET :server/trend

#POST team 1 no score
POST :server/term?team=team1&searchTerm=nmnrsgkregnrms

#POST team 2 no score
POST :server/term?team=team2&searchTerm=nrgreskjrengjkre

#GET trend-data
GET :server/trend

#GET start game
GET :server/start